{
  "name": "@raviqqe/loscore",
  "version": "0.1.2",
  "description": "Yet another utility library",
  "repository": "https://github.com/raviqqe/loscore",
  "license": "MIT",
  "publishConfig": {
    "provenance": true
  },
  "type": "module",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "scripts": {
    "build": "npm run build:tsc && npm run build:esbuild",
    "build:esbuild": "esbuild --minify --format=esm --sourcemap --outdir=dist 'src/*.ts'",
    "build:tsc": "tsc --emitDeclarationOnly",
    "prepare": "npm run build",
    "lint": "eslint 'src/**/*.ts'",
    "test": "vitest --coverage"
  },
  "devDependencies": {
    "@raviqqe/eslint-config": "^1.0.22",
    "@raviqqe/tsconfig": "^1.0.1",
    "@vitest/coverage-v8": "^0.34.1",
    "esbuild": "^0.19.5",
    "eslint": "^8.34.0",
    "typescript": "^5.0.2",
    "vitest": "^0.34.1"
  }
}
