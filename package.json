{
  "name": "@raviqqe/loscore",
  "version": "0.1.9",
  "description": "Yet another utility library",
  "repository": "https://github.com/raviqqe/loscore",
  "license": "MIT",
  "publishConfig": {
    "provenance": true
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "default": "./dist/main.js"
    },
    "./async": {
      "types": "./dist/async.d.ts",
      "default": "./dist/async.js"
    }
  },
  "scripts": {
    "build": "npm run build:tsc && npm run build:esbuild",
    "build:esbuild": "esbuild --minify --format=esm --sourcemap --outdir=dist $(git ls-files src)",
    "build:doc": "typedoc --out doc src/main.ts src/async.ts",
    "build:tsc": "tsc --emitDeclarationOnly",
    "prepare": "npm run build",
    "lint": "eslint 'src/**/*.ts' && npm run lint:knip",
    "lint:knip": "knip",
    "test": "vitest --coverage"
  },
  "devDependencies": {
    "@raviqqe/eslint-config": "^1.0.22",
    "@raviqqe/tsconfig": "^1.0.1",
    "@vitest/coverage-v8": "^0.34.1",
    "esbuild": "^0.19.5",
    "eslint": "^8.34.0",
    "knip": "^2.41.0",
    "typedoc": "^0.25.3",
    "typescript": "^5.0.2",
    "vitest": "^0.34.1"
  }
}
